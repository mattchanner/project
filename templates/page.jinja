<!doctype html>
<html lang="en-gb">
    <head>
        <title>{{name}}</title>
        <link rel="stylesheet" href="../../webclient/summary.css">
        <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
        <script
            src="https://maps.googleapis.com/maps/api/js?key={{apikey}}&callback=initMap&libraries=&v=weekly"
            defer></script>
    </head>
    <body>
        <div id="container">
            {{summary_table}}
        </div>
        <div id="map-container">
            <div id="map"></div>
        </div>
        <div id="img-container">
            <img src="{{filename}}" />

            {% for extension_chart in extension_charts %}
            <img src="{{extension_chart}}" />
            {% endfor %}
        </div>
    </body>
    <script>
        let points = JSON.parse('{{points}}');
    </script>
    <script src="../../webclient/app.js"></script>
</html>
